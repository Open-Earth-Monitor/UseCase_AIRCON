{
  "description": "Image collection format for Sentinel 5P data from Copernicus Dataspace Ecosystem",
  "tags": ["Sentinel", "Copernicus", "S5P"],
  "pattern": ".+\\.nc",
  "images": {
    "pattern": "..*\\/?(S5P_\\w{4}_L\\d[AB]?).+_(\\d{8}T\\d{6}_\\d{8}T\\d{6}_\\d{5}_\\d{2}_\\d{6}_\\d{8}T\\d{6})\\.nc"
  },
  "datetime": {
    "pattern": ".+_(\\d{8}T\\d{6})_\\d{8}T\\d{6}_\\d{5}_\\d{2}_\\d{6}_\\d{8}T\\d{6}\\.nc",
    "format": "%Y%m%dT%H%M%S"
  },
  "bands": {
    "SO2": {
      "pattern": ".+_L\\d[AB]?__?(SO2)_.*\\.nc"
    },
    "NO2": {
      "pattern": ".+_L\\d[AB]?__?(NO2)_.*\\.nc"
    },
    "O3": {
      "pattern": ".+_L\\d[AB]?__?(O3)_.*\\.nc"
    },
    "CO": {
      "pattern": ".+_L\\d[AB]?__?(CO)_.*\\.nc"
    },
    "CH4": {
      "pattern": ".+_L\\d[AB]?__?(CH4)_.*\\.nc"
    },
    "AER_AI": {
      "pattern": ".+_L\\d[AB]?__?(AER_AI)_.*\\.nc"
    },
    "CLOUD": {
      "pattern": ".+_L\\d[AB]?__?(CLOUD)_.*\\.nc"
    },
    "HCHO": {
      "pattern": ".+_L\\d[AB]?__?(HCHO)_.*\\.nc"
    }
  },
  "processing_stream": {
    "pattern": "S5P_(\\w{4})_"
  },
  "processing_level": {
    "pattern": ".+_(L\\d[AB]?)_"
  }
}
